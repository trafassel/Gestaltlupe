<DD>
using System;<BR>
using System.Collections.Generic;<BR>
using System.Linq;<BR>
using System.Text;<BR>
using System.Threading.Tasks;<BR>
<BR>
namespace Fractrace.Scheduler<BR>
{<BR>
    public class Scheduler<BR>
    {<BR>
<BR>
        protected Scheduler parent = null;<BR>
<BR>
        protected long time;<BR>
<BR>
        protected List&lt;Scheduler&gt; children = new List&lt;Scheduler&gt;();<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// Lock while update time.<BR>
        /// &lt;/summary&gt;<BR>
        protected Object timeLock = new Object();<BR>
<BR>
<BR>
        public Scheduler(Scheduler parent)<BR>
        {<BR>
            this.parent = parent;<BR>
        }<BR>
<BR>
<BR>
        public Scheduler()<BR>
        {<BR>
           <BR>
        }<BR>
<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// <BR>
        /// &lt;/summary&gt;<BR>
        public void Tick()<BR>
        {<BR>
            lock (timeLock)<BR>
            {<BR>
                ++time;<BR>
            }<BR>
        }<BR>
<BR>
<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// Called from an Tick event of a subentry.<BR>
        /// &lt;/summary&gt;<BR>
        public void PropagateTickUp(string eventName)<BR>
        {<BR>
            lock (timeLock)<BR>
            {<BR>
                ++time;<BR>
                if (parent != null)<BR>
                    parent.PropagateTickUp(eventName);<BR>
            }<BR>
        }<BR>
<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// Called from a tick event of parent.<BR>
        /// &lt;/summary&gt;<BR>
        public virtual void PropagateTickDown(string eventName,string value)<BR>
        {<BR>
            // Override this event, if neccessary. <BR>
        }<BR>
<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// <BR>
        /// &lt;/summary&gt;<BR>
        /// &lt;param name=&quot;name&quot;&gt;&lt;/param&gt;<BR>
        /// &lt;param name=&quot;val&quot;&gt;&lt;/param&gt;<BR>
        public virtual void Event(string name, string val)<BR>
        {<BR>
<BR>
        }<BR>
<BR>
<BR>
    }<BR>
}<BR>

</DD>
