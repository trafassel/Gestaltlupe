<DD>
// Problem: xx,yy,zz und r werden mehrmals ausgerechnet (Performance)<BR>
<BR>
int _updateColorCount=0;<BR>
<BR>
void UpdateColor(double x,double y,double z)<BR>
{<BR>
  _updateColorCount++;<BR>
  if (_updateColorCount &gt; _cycles / 3 &amp;&amp; _updateColorCount &lt; _cycles / 1.2)<BR>
  {<BR>
   double xx=x*x;<BR>
   double yy=y*y;<BR>
   double zz=z*z;<BR>
   double r=Math.Sqrt(xx+yy+zz);<BR>
    additionalPointInfo.red += xx / r;<BR>
    additionalPointInfo.green += yy / r;<BR>
    additionalPointInfo.blue += zz / r;<BR>
  }<BR>
}<BR>
<BR>
<BR>

</DD>
