<!--property:date_modified=1431371565-->
<!--property:hash=1717f14c2b446b248d1df6c267d7a63a25b016fb-->
<!--property:hash_html=7b33056b34e9527e42c28b806c80c0bcd8ccdb06-->
<DD>
http://en.wikipedia.org/wiki/B%C3%A9zier_curve<BR>
<BR>
P0=h(t);<BR>
P1=h(t)+( (h(t)-h(t-1) )*1.0/3.0;<BR>
P2=h(t+1)-( (h(t+2)-h(t+1) )*1.0/3.0;<BR>
P3=h(t+1)<BR>
<BR>
0&lt;=t&lt;=1:<BR>
<BR>
tm=(1-t);<BR>
tm2=tm*tm;<BR>
t2=t*t;<BR>
B(t)=tm2*tm*P0+3*tm2*t*P1+3*tm*t2*P2+t2*t*P3;<BR>
<BR>
<BR>
